{
    "name": "florianv/swap",
    "type": "library",
    "description": "Exchange rates library for PHP",
    "keywords": ["currency", "money", "rate", "conversion", "exchange rates"],
    "homepage": "https://github.com/florianv/swap",
    "license": "MIT",
    "authors": [
        {
            "name": "Florian Voutzinos",
            "email": "florian@voutzinos.com",
            "homepage": "http://florian.voutzinos.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Swap\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Swap\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.4.0",
        "egeloen/http-adapter": "~1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "doctrine/cache": "~1.0",
        "illuminate/cache": "~5.0"
    },
    "suggest": {
        "doctrine/cache": "Required to cache rates with Doctrine cache",
        "illuminate/cache": "Required to cache rates with Illuminate cache"
    },
    "config": {
        "bin-dir": "bin"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
